{
  "version": 8,
  "name": "Empty Style",
  "metadata": {
    "maputnik:renderer": "mlgljs"
  },
  "sources": {
    "raster": {
      "maxzoom": 19,
      "minzoom": 0,
      "scheme": "xyz",
      "tileSize": 256,
      "tiles": ["https://tile.openstreetmap.de/{z}/{x}/{y}.png"],
      "type": "raster"
    },
    "indoor": {
      "type": "vector",
      "url": "https://nav.tum.de/martin/indoor_rooms,indoor_pois,indoor_walls,indoor_doors?level=-1.0"
    }
  },
  "sprite": "https://nav.tum.de/martin/sdf_sprite/maki,navigatum",
  "glyphs": "https://nav.tum.de/martin/font/{fontstack}/{range}",
  "layers": [
    {
      "id": "base",
      "type": "raster",
      "source": "raster"
    },
    {
      "id": "buildings-background",
      "type": "fill",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 16.5,
      "filter": ["any", ["has", "building"], ["has", "building:part"]],
      "paint": {
        "fill-color": "#FFFFFF",
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 16.5, 0, 18, 1]
      }
    },
    {
      "id": "indoor-rooms",
      "type": "fill",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 16,
      "filter": ["all", ["!=", "indoor", "level"]],
      "paint": {
        "fill-color": [
          "match",
          ["get", "indoor"],
          "room",
          "#e0e0e0",
          "corridor",
          "#b9e1ff",
          "stairs",
          "#b9e1ff",
          "elevator",
          "#b9e1ff",
          "toilet",
          "#E6C7E1",
          "shower",
          "#FCE2B0",
          "area",
          "#8dd1fc",
          "wall",
          "#fff",
          "#cccaca"
        ],
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 16.5, 0, 17, 1],
        "fill-antialias": false
      },
      "layout": {
        "visibility": "visible"
      }
    },
    {
      "id": "indoor-walls",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_walls",
      "minzoom": 16.5,
      "filter": ["all"],
      "paint": {
        "line-color": "#000000",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 16.5, 0, 18, 1],
        "line-width": {
          "stops": [[16, 0], [17, 0.1], [23, 6]]
        }
      },
      "layout": {
        "line-join": "round",
        "line-cap": "butt"
      }
    },
    {
      "id": "indoor-barriers",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "filter": ["has", "barrier"],
      "paint": {
        "line-color": "#000000",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 18, 1]
      }
    },
    {
      "id": "indoor-handrail",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "filter": ["all", ["==", ["get", "handrail"], "yes"], ["!=", ["get", "highway"], "steps"]],
      "paint": {
        "line-color": "#000000",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 19, 1]
      }
    },
    {
      "id": "indoor-pois",
      "type": "symbol",
      "source": "indoor",
      "minzoom": 17,
      "filter": ["all"],
      "layout": {
        "text-allow-overlap": {
          "stops": [[6, false], [10, false]]
        },
        "text-anchor": "center",
        "text-field": [
          "case",
          ["all", ["has", "name"], ["has", "ref"]],
          ["format", ["get", "name"], {}, "\n", {}, ["get", "ref"], { "font-scale": 0.8 }],
          ["has", "name"],
          ["get", "name"],
          ["get", "ref"]
        ],
        "text-font": ["Roboto Regular"],
        "text-ignore-placement": false,
        "text-justify": "auto",
        "text-rotation-alignment": "auto",
        "text-size": ["interpolate", ["exponential", 1.5], ["zoom"], 16.5, 8, 24, 18],
        "icon-optional": false,
        "icon-allow-overlap": false,
        "symbol-placement": "point",
        "text-pitch-alignment": "auto",
        "text-overlap": "cooperative",
        "text-optional": true,
        "symbol-sort-key": ["get", "area"],
        "symbol-avoid-edges": false,
        "icon-image": [
          "case",
          ["==", ["get", "indoor"], "elevator"],
          "elevator",
          ["get", "is_disabled_toilet"],
          "wheelchair",
          ["get", "is_unisex_toilet"],
          "toilet",
          ["get", "is_male_toilet"],
          "toilet_male",
          ["get", "is_female_toilet"],
          "toilet_female",
          ["get", "is_shower"],
          "noun-shower-51",
          ""
        ],
        "icon-overlap": "never",
        "icon-size": {
          "stops": [[17, 0.5], [22, 6]],
          "base": 2
        }
      },
      "paint": {
        "text-color": "#072140",
        "text-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          17,
          0,
          17.5,
          ["case", [">", ["get", "area"], 100], 1, 0],
          18.5,
          ["case", [">", ["get", "area"], 25], 1, 0],
          19.5,
          ["case", [">", ["get", "area"], 5], 1, 0],
          20.5,
          1
        ],
        "icon-color": [
          "case",
          ["==", ["get", "indoor"], "elevator"],
          "#072140",
          ["==", ["get", "indoor"], "shower"],
          "#D99208",
          "#9B468D"
        ]
      },
      "source-layer": "indoor_pois"
    },
    {
      "id": "indoorsteps-white-base",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 17,
      "filter": ["all", ["==", ["get", "highway"], "steps"], ["!", ["has", "conveying"]]],
      "layout": { "line-join": "round" },
      "paint": {
        "line-color": "rgba(255, 252, 252, 0.8)",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.25, 1],
        "line-width": [
          "interpolate",
          ["exponential", 2],
          ["zoom"],
          10,
          ["*", 1, ["^", 2, -6]],
          24,
          ["*", 1, ["^", 2, 8]]
        ]
      }
    },
    {
      "id": "indoorsteps-base",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 17,
      "filter": ["all", ["==", ["get", "highway"], "steps"], ["!", ["has", "conveying"]]],
      "layout": { "line-join": "round" },
      "paint": {
        "line-color": "rgba(0, 0, 0, 1)",
        "line-gap-width": [
          "interpolate",
          ["exponential", 2],
          ["zoom"],
          10,
          ["*", 1, ["^", 2, -6]],
          24,
          ["*", 1, ["^", 2, 8]]
        ],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.25, 1],
        "line-width": [
          "interpolate",
          ["exponential", 2],
          ["zoom"],
          10,
          ["*", 0.1, ["^", 2, -6]],
          24,
          ["*", 0.1, ["^", 2, 8]]
        ]
      }
    },
    {
      "id": "indoorsteps-black",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 17,
      "filter": ["all", ["all", ["==", ["get", "highway"], "steps"], ["!", ["has", "conveying"]]]],
      "layout": { "line-join": "round" },
      "paint": {
        "line-color": "rgba(0, 0, 0, 1)",
        "line-dasharray": [
          "step",
          ["zoom"],
          ["literal", [0, 1]],
          20,
          ["literal", [0.1, 0.4]],
          21,
          ["literal", [0.1, 0.3]],
          22,
          ["literal", [0.05, 0.2]]
        ],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.25, 1],
        "line-width": [
          "interpolate",
          ["exponential", 2],
          ["zoom"],
          10,
          ["*", 1, ["^", 2, -6]],
          24,
          ["*", 1, ["^", 2, 8]]
        ]
      }
    },
    {
      "id": "indoorsteps-middle",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 17,
      "filter": ["all", ["all", ["==", ["get", "highway"], "steps"], ["!", ["has", "conveying"]]]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "rgba(22, 93, 177, 0.4)",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.25, 1],
        "line-width": [
          "interpolate",
          ["exponential", 1.8],
          ["zoom"],
          17,
          ["*", 1, ["^", 2, -2]],
          24,
          ["*", 1, ["^", 2, 6]]
        ]
      }
    },
    {
      "id": "indoorsteps-arrow",
      "type": "symbol",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 18,
      "filter": ["==", ["get", "highway"], "steps"],
      "layout": {
        "icon-allow-overlap": true,
        "icon-ignore-placement": false,
        "icon-image": "triangle",
        "icon-keep-upright": false,
        "icon-rotate": 90,
        "icon-size": [
          "interpolate",
          ["exponential", 1.8],
          ["zoom"],
          17,
          ["*", 1, ["^", 2, -2]],
          24,
          ["*", 1, ["^", 2, 3]]
        ],
        "symbol-avoid-edges": true,
        "symbol-placement": "line"
      },
      "paint": {
        "icon-color": "rgba(22, 93, 177, 0.4)",
        "icon-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.5, 0.5, 19, 1]
      }
    },
    {
      "id": "indoorsteps-conveying",
      "type": "line",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "filter": ["all", ["==", ["get", "highway"], "steps"], ["has", "conveying"]],
      "layout": { "line-join": "round" },
      "paint": {
        "line-color": "#FF0000",
        "line-dasharray": [
          "step",
          ["zoom"],
          ["literal", [1, 0]],
          17.5,
          ["literal", [1.75, 1]],
          18,
          ["literal", [1, 0.75]],
          19,
          ["literal", [0.3, 0.3]]
        ],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.25, 1],
        "line-width": ["interpolate", ["exponential", 1.5], ["zoom"], 17, 1, 18, 1.6, 19, 6]
      }
    },
    {
      "id": "poi-furniture",
      "type": "fill",
      "source": "indoor",
      "source-layer": "indoor_pois",
      "minzoom": 18,
      "filter": [
        "any",
        [
          "in",
          ["get", "room"],
          [
            "literal",
            ["table", "cupboard", "chair", "kitchen", "sofa", "tv", "shelf", "furniture-item"]
          ]
        ],
        ["==", ["get", "trashcan"], "yes"],
        ["==", ["get", "copier"], "yes"],
        ["==", ["get", "amenity"], "vending_machine"]
      ],
      "paint": {
        "fill-color": "#000",
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 18, 0, 19, 0.2],
        "fill-outline-color": "#000"
      }
    },
    {
      "id": "poi-exterior-exit-bg",
      "type": "circle",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 18,
      "filter": [
        "all",
        ["any", ["==", ["get", "entrance"], "exit"], ["==", ["get", "entrance"], "emergency"]]
      ],
      "paint": {
        "circle-color": "rgba(255, 255, 255, 1)",
        "circle-opacity": ["interpolate", ["linear"], ["zoom"], 18, 0, 18.5, 0.8],
        "circle-radius": 10
      }
    },
    {
      "id": "poi-exterior-exit",
      "type": "symbol",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 18,
      "filter": [
        "all",
        ["any", ["==", ["get", "entrance"], "exit"], ["==", ["get", "entrance"], "emergency"]]
      ],
      "layout": {
        "icon-image": "exit-alt1"
      },
      "paint": {
        "icon-opacity": ["interpolate", ["linear"], ["zoom"], 18, 0, 18.5, 1]
      }
    },
    {
      "id": "poi-exterior-entrance-bg",
      "type": "circle",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 16,
      "filter": [
        "all",
        ["has", "entrance"],
        ["!=", ["get", "entrance"], "exit"],
        ["!=", ["get", "entrance"], "emergency"]
      ],
      "paint": {
        "circle-color": "rgba(255, 255, 255, 1)",
        "circle-opacity": ["interpolate", ["linear"], ["zoom"], 16, 0, 16.5, 1],
        "circle-radius": 10
      }
    },
    {
      "id": "poi-exterior-entrance",
      "type": "symbol",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 16,
      "filter": [
        "all",
        ["has", "entrance"],
        ["!=", ["get", "entrance"], "exit"],
        ["!=", ["get", "entrance"], "emergency"]
      ],
      "layout": {
        "icon-allow-overlap": true,
        "icon-ignore-placement": true,
        "icon-image": "entrance-alt1",
        "icon-optional": false,
        "text-font": []
      },
      "paint": {
        "icon-opacity": ["interpolate", ["linear"], ["zoom"], 16, 0, 16.5, 1]
      }
    },
    {
      "id": "poi-indoor-bathrooms",
      "type": "symbol",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 18,
      "filter": ["in", ["get", "amenity"], ["literal", ["toilets", "shower", "showers"]]],
      "layout": {
        "icon-allow-overlap": true,
        "icon-anchor": "center",
        "icon-image": [
          "case",
          ["in", ["get", "amenity"], ["literal", ["shower", "showers"]]],
          ["literal", "noun-shower-51"],
          ["==", ["get", "wheelchair"], "yes"],
          ["literal", "wheelchair"],
          ["all", ["==", ["get", "female"], "yes"], ["!=", ["get", "male"], "yes"]],
          ["literal", "toilet_female"],
          ["all", ["!=", ["get", "female"], "yes"], ["==", ["get", "male"], "yes"]],
          ["literal", "toilet_male"],
          ["literal", "toilet"]
        ],
        "icon-size": ["interpolate", ["linear"], ["zoom"], 17, 0.5, 20, 1],
        "text-font": ["Roboto", " "],
        "text-overlap": "never",
        "text-transform": "uppercase",
        "text-ignore-placement": false,
        "text-optional": false
      },
      "paint": {
        "icon-opacity": ["interpolate", ["linear"], ["zoom"], 18, 0, 18.5, 0.5, 19, 1],
        "text-translate-anchor": "map"
      }
    },
    {
      "id": "poi-indoor",
      "type": "symbol",
      "source": "indoor",
      "source-layer": "indoor_rooms",
      "minzoom": 17,
      "filter": ["boolean", false],
      "layout": {
        "icon-allow-overlap": true,
        "icon-anchor": "center",
        "icon-image": ["get", "maki"],
        "icon-optional": false,
        "icon-size": ["interpolate", ["linear"], ["zoom"], 17, 0.5, 20, 1],
        "symbol-placement": "point",
        "symbol-spacing": 250,
        "text-allow-overlap": false,
        "text-anchor": "top",
        "text-field": [
          "case",
          ["all", ["has", "name"], ["has", "name"]],
          ["format", ["get", "name"], {}, "\n", {}, ["get", "ref"], { "font-scale": 0.8 }],
          ["has", "name"],
          ["get", "name"],
          ["get", "ref"]
        ],
        "text-font": ["Roboto Regular"],
        "text-ignore-placement": false,
        "text-line-height": 1.2,
        "text-max-angle": 38,
        "text-max-width": 8,
        "text-offset": [0, 1],
        "text-padding": 2,
        "text-rotation-alignment": "viewport",
        "text-size": ["interpolate", ["linear"], ["zoom"], 17, 11, 20, 13]
      },
      "paint": {
        "icon-opacity": ["interpolate", ["linear"], ["zoom"], 18, 0, 18.5, 0.5, 19, 1],
        "text-color": "#072140",
        "text-halo-color": "rgb(194, 215, 239, 0.3)",
        "text-halo-width": 1,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 17, 0, 17.5, 0.5, 19, 1]
      }
    }
  ],
  "id": "rmt811v"
}
