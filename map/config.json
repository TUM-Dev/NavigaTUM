{
  "options": {
    "paths": {
      "root": "",
      "fonts": "fonts",
      "sprites": "sprites",
      "styles": "styles",
      "mbtiles": ""
    },
    "domains": [
      "localhost:8080",
      "127.0.0.1:8080"
    ],
    "formatQuality": {
      "jpeg": 80,
      "webp": 90
    },
    "maxScaleFactor": 2,
    "maxRendererPoolSizes": [4],
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": true,
    "serveAllStyles": true,
    "serveStaticMaps": true,
    "tileMargin": 0
  },
  "styles": {
    "osm_liberty": {
      "style": "osm_liberty.json",
      "serve_rendered": true,
      "tilejson": {
        "format": "png"
      }
    }
  },
  "data": {
    "openmaptiles": {
      "mbtiles": "output.mbtiles"
    }
  }
}
