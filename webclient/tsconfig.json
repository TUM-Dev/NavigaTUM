{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "**/*.ts", "src/**/*.vue"],
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "types": ["cypress", "@intlify/unplugin-vue-i18n/messages"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["es5", "esnext", "dom", "dom.iterable"]
  }
}
