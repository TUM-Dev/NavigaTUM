{
  "name": "navigatum",
  "version": "0.1.0",
  "scripts": {
    "cy:run:chrome:e2e": "cypress run --browser chrome --e2e",
    "cy:run:firefox:e2e": "cypress run --browser firefox:dev --e2e",
    "cy:run:chrome:component": "cypress run --browser chrome --component",
    "cy:run:firefox:component": "cypress run --browser firefox:dev --component",
    "cy:open:components": "cypress open-ct --browser firefox:dev",
    "cy:open:e2e": "cypress open --browser firefox:dev --e2e",
    "test": "run-p cy:run:firefox:e2e cy:run:chrome:e2e",
    "test:components": "npm run cy:run:chrome:component && npm run cy:run:firefox:component",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 3000",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --plugin=prettier-plugin-tailwindcss --write ."
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@heroicons/vue": "2.1.1",
    "@vueuse/core": "10.7.2",
    "maplibre-gl": "4.0.1",
    "markdown-it-prism": "2.3.0",
    "pinia": "2.1.7",
    "swagger-ui-dist": "5.11.6",
    "swaggerdark": "github:octycs/SwaggerDark#f02d394c8ff698cdd93e09c2188b058d2d686ca3",
    "unplugin-vue-markdown": "0.26.0",
    "vue": "3.4.19",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "2.0.0",
    "@rushstack/eslint-patch": "1.7.2",
    "@types/swagger-ui-dist": "3.30.4",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.17",
    "cypress": "13.6.4",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.22.0",
    "isomorphic-fetch": "3.0.0",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "sass": "1.70.0",
    "stylelint-scss": "6.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.1.2",
    "vite-plugin-vue-devtools": "7.0.15",
    "vue-i18n": "9.9.1",
    "vue-tsc": "1.8.27"
  },
  "type": "module"
}
