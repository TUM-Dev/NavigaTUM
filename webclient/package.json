{
  "name": "navigatum",
  "version": "0.1.0",
  "scripts": {
    "cy:run:chrome:e2e": "cypress run --browser chrome --e2e",
    "cy:run:firefox:e2e": "cypress run --browser firefox:dev --e2e",
    "cy:run:chrome:component": "cypress run --browser chrome --component",
    "cy:run:firefox:component": "cypress run --browser firefox:dev --component",
    "cy:open:components": "cypress open-ct --browser firefox:dev",
    "cy:open:e2e": "cypress open --browser firefox:dev --e2e",
    "test": "run-p cy:run:firefox:e2e cy:run:chrome:e2e",
    "test:components": "npm run cy:run:chrome:component && npm run cy:run:firefox:component",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 3000",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@vueuse/core": "10.7.0",
    "maplibre-gl": "3.6.2",
    "markdown-it-prism": "^2.3.0",
    "pinia": "2.1.7",
    "spectre.css": "github:Valexr/spectre#dfe3bc2c59d23cd4bfd43c690aae3655576ff708",
    "swagger-ui-dist": "5.10.5",
    "swaggerdark": "github:octycs/SwaggerDark#f02d394c8ff698cdd93e09c2188b058d2d686ca3",
    "unplugin-vue-markdown": "^0.25.2",
    "vue": "3.3.13",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "2.0.0",
    "@rushstack/eslint-patch": "1.6.1",
    "@types/swagger-ui-dist": "3.30.4",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "^10.4.16",
    "cypress": "13.6.1",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.19.2",
    "isomorphic-fetch": "3.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.1.1",
    "sass": "1.69.5",
    "stylelint-scss": "6.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.3",
    "vite": "4.5.1",
    "vite-plugin-rewrite-all": "1.0.2",
    "vue-i18n": "9.8.0",
    "vue-tsc": "1.8.26"
  },
  "type": "module"
}
